{
	"name": "paperAgents",
	"version": "0.0.1",
	"description": "Paper Agents turns Obsidian into an agent sandbox: define, test, and execute workflows and tools in Markdown, with OpenRouter integration and sandboxed JavaScript.",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ."
	},
	"keywords": [
		"obsidian",
		"plugin",
		"ai",
		"agents",
		"markdown",
		"openrouter",
		"workflow"
	],
	"license": "MIT",
	"author": {
		"name": "Merlin Becker",
		"url": "https://github.com/merlinbecker/PaperAgents"
	},
	"homepage": "https://github.com/merlinbecker/PaperAgents#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/merlinbecker/PaperAgents.git"
	},
	"bugs": {
		"url": "https://github.com/merlinbecker/PaperAgents/issues"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"@eslint/js": "9.30.1",
		"jiti": "2.6.1"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}
